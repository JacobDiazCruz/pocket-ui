{
  "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "declaration": false,
      "outDir": "./dist",
      "baseUrl": "./src"
  },
  "include": ["src/**/*.tsx"]
}
